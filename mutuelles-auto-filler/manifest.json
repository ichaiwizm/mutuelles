{
  "manifest_version": 3,
  "name": "SwissLife Orchestrateur",
  "version": "2.0",
  "description": "Orchestrateur modulaire pour remplissage SwissLife",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*.swisslifeone.fr/*",
    "http://localhost:5174/*"
  ],
  "action": {
    "default_title": "SwissLife Orchestrateur"
  },
  "content_scripts": [
    {
      "matches": ["https://*.swisslifeone.fr/*", "http://localhost:5174/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "data/test-data.json",
        "data/swisslife-rules.json",
        "data/swisslife-mappings.json",
        "src/ui/ui.js",
        "src/core/orchestrator.js", 
        "src/core/template-processor.js",
        "src/core/dependency-resolver.js",
        "src/core/iframe-bridge.js",
        "services/swisslife/orchestrator-bridge.js",
        "services/swisslife/fields/*.js",
        "services/swisslife/fields/**/*.js",
        "services/swisslife/utils/*.js",
        "services/swisslife/navigation/*.js"
      ],
      "matches": ["https://*.swisslifeone.fr/*"]
    }
  ]
}