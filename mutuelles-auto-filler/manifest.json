{
  "manifest_version": 3,
  "name": "Mutuelles Orchestrateur",
  "version": "3.0",
  "description": "Orchestrateur multi-providers (fenÃªtre unique, pool d'onglets)",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://*.swisslifeone.fr/*",
    "http://localhost:5174/*",
    "https://mutuelles-lead-extractor.vercel.app/*"
  ],
  "action": {
    "default_title": "Mutuelles Orchestrateur"
  },
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["http://localhost:5174/*", "https://mutuelles-lead-extractor.vercel.app/*"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.swisslifeone.fr/*", "http://localhost:5174/*", "https://mutuelles-lead-extractor.vercel.app/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "data/swisslife-rules.json",
        "data/swisslife-mappings.json",
        "src/config/*.js",
        "src/ui/ui.js",
        "src/core/*.js",
        "src/core/*/*.js",
        "src/core/*/*/*.js",
        "src/core/*/*/*/*.js",
        "src/providers/*.js",
        "src/providers/*/*.js",
        "src/content/*.js",
        "services/swisslife/*.js",
        "services/swisslife/*/*.js",
        "services/swisslife/*/*/*.js",
        "services/swisslife/*/*/*/*.js",
        "services/swisslife/fields/*.js",
        "services/swisslife/fields/*/*.js",
        "services/swisslife/utils/*.js",
        "services/swisslife/navigation/*.js",
        "background.js"
      ],
      "matches": ["https://*.swisslifeone.fr/*", "http://localhost:5174/*", "https://mutuelles-lead-extractor.vercel.app/*"]
    }
  ]
}
