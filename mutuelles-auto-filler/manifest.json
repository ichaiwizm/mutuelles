{
  "manifest_version": 3,
  "name": "Mutuelles Auto-Filler",
  "version": "3.0",
  "description": "Extension de remplissage automatique pour les sites de mutuelles",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*.swisslifeone.fr/*",
    "http://localhost:5173/*"
  ],
  "action": {
    "default_title": "Mutuelles Auto-Filler"
  },
  "content_scripts": [
    {
      "matches": ["https://*.swisslifeone.fr/*"],
      "js": [
        "services/swisslife/bigtest.js",
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}