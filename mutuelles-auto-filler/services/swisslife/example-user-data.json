    ],
    "execution": [
      "Exécuter le workflow correspondant au scenario",
      "Vérifier chaque section après remplissage",
      "Valider les dépendances entre champs",
      "Tester les mécanismes de fallback"
    ],
    "validation": [
      "Contrôler la cohérence des données saisies",
      "Vérifier l'activation/désactivation des sections conditionnelles",
      "Tester la gestion d'erreurs sur champs invalides",
