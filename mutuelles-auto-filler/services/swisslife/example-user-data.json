  "validationRules": {
    "dateNaissance": {
      "format": "DD/MM/YYYY",
      "minAge": 16,
      "maxAge": 80
    },
    "departement": {
      "validCodes": ["01", "02", "03", "74", "75", "69", "13", "06", "67", "82", "2A", "2B"],
      "pattern": "^(\\d{2}|2[AB])$"
    },
    "children": {
      "maxNumber": 10,
      "maxAgeYears": 25
    }
  },
  
  "testingInstructions": {
    "setup": [
